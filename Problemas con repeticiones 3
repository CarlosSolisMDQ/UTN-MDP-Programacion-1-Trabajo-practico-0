#include <stdio.h>
#include <stdlib.h>


int main()
{
    int num, cubo, cuarta, counter;
    int arr[10];
    counter = 0;
    
    //leer 10 numeros e imprimir solo los positivos.
    
    do {
        printf("ingrese un numero:\n");
        scanf("%d", &arr[counter]);

        counter++;
    } while (counter != 10);

    for(int i = 0; i <10; i++) {
        if ( arr[i] > 0) {
            printf("%d", arr[i]);
        }
    }


    return 0;
}
